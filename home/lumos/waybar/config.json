{
  "layer": "top",
  "position": "top",
  "height": 30,
  "modules-left": ["hyprland/workspaces"],
  "modules-center": ["hyprland/window"],
  "modules-right": ["clock", "battery", "tray"],
  "hyprland/workspaces": {
    "format": "{name}",
    "on-click": "activate"
  },
  "clock": {
    "format": "{:%H:%M %a %d-%m}"
  },
  "battery": {
    "bat": "BAT0",
    "format": "{icon} {capacity}%",
    "format-icons": ["", "", "", "", ""]
  }
}